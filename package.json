{
  "name": "popcorn-frontend",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:fresh": "if lsof -ti:3000 >/dev/null 2>&1; then lsof -ti:3000 | xargs kill -9; fi; rm -rf .next-dev .next && next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5"
  }
}
