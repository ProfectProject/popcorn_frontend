{
  "name": "popcorn-frontend",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:fresh": "if lsof -ti:3000 >/dev/null 2>&1; then lsof -ti:3000 | xargs kill -9; fi; rm -rf .next-dev .next && next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint . --ext .js,.jsx --max-warnings=0"
  },
  "dependencies": {
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^25.3.0",
    "@types/react": "19.2.14",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "typescript": "^5.9.3"
  }
}
